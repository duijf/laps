cabal-version:       >=1.10
name:                laps
version:             0.0.1.0
author:              Laurens Duijvesteijn
maintainer:          git@duijf.io
build-type:          Simple

executable laps
  main-is:             Main.hs
  build-depends:       base >=4.13 && <4.14
  default-language:    Haskell2010
  default-extensions:
    DeriveGeneric,
    FlexibleContexts,
    LambdaCase,
    OverloadedStrings,
    TypeApplications
